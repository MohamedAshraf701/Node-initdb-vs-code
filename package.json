{
  "name": "node-initdb",
  "displayName": "node-initdb",
  "description": "node-initdb is a Vs Code Extension for initializing database configurations and structures in Node.js projects.",
  "version": "0.0.6",
  "publisher": "MohamedAshraf",
  "icon": "Logo.png",
  "engines": {
    "vscode": "^1.95.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "https://github.com/MohamedAshraf701/Node-initdb-vs-code.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "node-initdb.setupDatabase",
        "title": "Node-initdb : Setup Project"
      },
      {
        "command": "node-initdb.addModule",
        "title": "Node-initdb : Add Module"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "mkdirp": "^3.0.1",
    "path": "^0.12.7"
  }
}
